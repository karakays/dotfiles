[user]
	email = skarakayali@gmail.com
	name = Selçuk Karakayalı
[core]
	editor = vim
	excludesfile = .gitignore_global
	autocrlf = false
[commit]
    template = .stCommitMsg
[alias]
    logg = log --all --decorate --abbrev-commit --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset'
    st = status
[color]
	ui = true
